@model List<LaptopObject>

<div class="row">

    <h1> Cheapest Laptops</h1>
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Brand</th>
                    <th scope="col">Model</th>
                    <th scope="col">Price</th>
                    <th scope="col">Year</th>
                    <th scope="col">Id</th>
                </tr>
            </thead>
            @foreach (var laptop in Model)
            {
                <tbody>
                    <tr>
                        <td>@laptop.BrandName</td>
                        <td>@laptop.Model</td>
                        <td>@laptop.Price</td>
                        <td>@laptop.Year</td>
                        <td>@laptop.Id</td>
                    </tr>
                </tbody>
            }
        </table>
    </div>
</div>